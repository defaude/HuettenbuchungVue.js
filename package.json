{
  "name": "huettenbuchung",
  "version": "1.0.0",
  "description": "Minimal Vue.js app template using Steal.js",
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production steal-tools --bundle-steal --envify",
    "start": "http-server -o -c-1 ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/weser/HuettenbuchungVue.js.git"
  },
  "author": "Dr. Martin Weser",
  "private": true,
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/weser/HuettenbuchungVue.js/issues"
  },
  "homepage": "https://github.com/weser/HuettenbuchungVue.js#readme",
  "dependencies": {
    "steal": "^1.5.15",
    "vue": "^2.5.2"
  },
  "devDependencies": {
    "cross-env": "^5.1.0",
    "http-server": "^0.10.0",
    "steal-css": "^1.3.1",
    "steal-tools": "^1.9.1"
  },
  "steal": {
    "map": {
      "vue": "vue/dist/vue.common"
    },
    "plugins": [
      "steal-css"
    ],
    "ext": {
      "html": "load-html"
    }
  }
}
